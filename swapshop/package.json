{
  "name": "swapshop",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --coverage",
    "badges": "merge-markdown -m ./readme-config/readme.json && jest-coverage-badges && coverage-badge-creator"
